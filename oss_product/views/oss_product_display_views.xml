		<record id="oss_product_form" model="ir.ui.view">
			<field name="name">oss.product.form</field>
			<field name="model">oss.product</field>
			<field name="arch" type="xml">
				<form string="Product">
					<sheet>
						<group>
							<field name="name"/>
							<field name="description"/>
							<field name="family_name"/>
							<field name="category"/>
							<field name="ptype"/>
							<!-- <field name="group_name"/> -->
							<field name="product_picture" widget="image" class="oe_avatar"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_product_group_form" model="ir.ui.view">
			<field name="name">oss_product.group.form</field>
			<field name="model">oss_product.group</field>
			<field name="arch" type="xml">
				<form string="Product Group">
					<sheet>
						<group>
							<field name="name"/>
							<field name="description"/>
							<field name="family_name"/>
							<field name="category"/>
							<field name="ptype"/>
							<field name="location"/>
							<field name="picture" widget="image" class="oe_avatar"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_location_form" model="ir.ui.view">
			<field name="name">oss.location.form</field>
			<field name="model">oss.location</field>
			<field name="arch" type="xml">
				<form string="Location">
					<sheet>
						<group>
							<field name="country"/>
							<field name="province_state"/>
							<field name="city"/>
							<field name="street"/>
							<field name="zip_code"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_varient_form" model="ir.ui.view">
			<field name="name">oss.varient.form</field>
			<field name="model">oss.varient</field>
			<field name="arch" type="xml">
				<form string="Varient">
					<sheet>
						<group>
							<field name="varient"/>
							<field name="product"/>
							<field name="description"/>
							<field name="vtype"/>
							<field name="category"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_varient_config_group_form" model="ir.ui.view">
			<field name="name">oss_varient.config.group.form</field>
			<field name="model">oss_varient.config.group</field>
			<field name="arch" type="xml">
				<form string="Varient Config Group">
					<sheet>
						<group>
							<field name="variant_config_name"/>
							<field name="variant" widget="many2many_tags"/>
							<field name="vgtype"/>
							<field name="category"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_config_item_form" model="ir.ui.view">
			<field name="name">oss_config.item.form</field>
			<field name="model">oss_config.item</field>
			<field name="arch" type="xml">
				<form string="Config Item">
					<sheet>
						<group>
							<group>
								<field name="config_item"/>
								<field name="description"/>
								<field name="varient_config"/>
								<field name="parameter_type"/>
							</group>
							<group>
								<field name="measurement_unit"/>
								<field name="is_priced"/>
								<field name="currency_unit"/>
								<field name="unit_price"/>
							</group>
							<group>
								<field name="minimun_limit"/>
								<field name="maximum_limit"/>
								<field name="included_in_base_price"/>
								<field name="renewable"/>
								<field name="renew_frequency"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_config_options_form" model="ir.ui.view">
