<!-- List/Tree Views-->
		<record id="product_family_category_list" model="ir.ui.view">
			<field name="name">oss_product.family.category.list</field>
			<field name="model">oss_product.family.category</field>
			<field name="arch" type="xml">
				<tree string="Product Family Category">
					<field name="name"/>
					<field name="description"/>
				</tree>
			</field>
		</record>

		<record id="product_family_type_list" model="ir.ui.view">
			<field name="name">oss_product.family.type.list</field>
			<field name="model">oss_product.family.type</field>
			<field name="arch" type="xml">
				<tree string="Product Family type">
					<field name="name"/>
					<field name="description"/>
				</tree>
			</field>
		</record>

		<record id="product_family_list" model="ir.ui.view">
			<field name="name">oss_product.family.list</field>
			<field name="model">oss_product.family</field>
			<field name="arch" type="xml">
				<tree string="Product Family">
					<field name="name"/>
					<field name="description"/>
					<field name="category"/>
					<field name="ptype"/>
					<!-- <field name="image" widget="image" class="oe_avatar"/> -->
				</tree>
			</field>
		</record>

		<record id="oss_product_group_category_list" model="ir.ui.view">
			<field name="name">oss_product.group.category.list</field>
			<field name="model">oss_product.group.category</field>
			<field name="arch" type="xml">
				<tree string="Product Group Category">
					<field name="name"/>
					<field name="description"/>
				</tree>
			</field>
		</record>
		<record id="oss_product_group_type_list" model="ir.ui.view">
			<field name="name">oss_product.group.type.list</field>
			<field name="model">oss_product.group.type</field>
			<field name="arch" type="xml">
				<tree string="Product Group Type">
					<field name="name"/>
					<field name="description"/>
				</tree>
			</field>
		</record>

<!-- from Views-->
		<record id="oss_product_family_category_form" model="ir.ui.view">
			<field name="name">oss_product.family.category.form</field>
			<field name="model">oss_product.family.category</field>
			<field name="arch" type="xml">
				<form string="Product Family Category">
					<sheet>
						<group>
							<field name="name"/>
							<field name="description"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="oss_product_family_type_form" model="ir.ui.view">
