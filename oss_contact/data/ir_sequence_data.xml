<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_enquiry" model="ir.sequence">
            <field name="name">Enquiry Generate sequence</field>
            <field name="code">enquiry_sequence</field>
            <field name="prefix">%(day)s%(month)s%(year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lead_generated_sequence" model="ir.sequence">
            <field name="name">Lead Generated Sequence</field>
            <field name="code">lead_generated_sequence</field>
            <field name="prefix">LEA%(y)s%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_lead_task" model="ir.sequence">
            <field name="name">Task Generate sequence</field>
            <field name="code">task_generated_sequence</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lead_stakeholder" model="ir.sequence">
            <field name="name">Stakeholder Generated Sequence</field>
            <field name="code">stakeholder_generated_sequence</field>
            <field name="prefix">LS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lead_application" model="ir.sequence">
            <field name="name">Application Generated Sequence</field>
            <field name="code">application_details_sequence</field>
            <field name="prefix">APP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_site" model="ir.sequence">
            <field name="name">Site Sequence</field>
            <field name="code">site_sequence</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
